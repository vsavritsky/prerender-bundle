{
    "name": "vsavritsky/prerender-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily use prerender for your Symfony apps running Client-Side MVC",
    "keywords": ["prerender","MVC js"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "^3.4.30|^4.3.3|^5.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-0": { "Vsavritsky\\PrerenderBundle": "" }
    },
    "minimum-stability": "stable",
    "target-dir": "Vsavritsky/PrerenderBundle"
}
